	<obj code = "flora"	name = "Tree 4"	x = "48"  y = "71" rotation_body = "0" rotation_head = "0" smooth = "true"/>
	<obj code = "flora"	name = "Tree 5"	x = "122" y = "65" rotation_body = "0" rotation_head = "0" smooth = "true"/>
	<obj code = "flora"	name = "Tree 6"	x = "122" y = "65" rotation_body = "0" rotation_head = "0" smooth = "true"/>
